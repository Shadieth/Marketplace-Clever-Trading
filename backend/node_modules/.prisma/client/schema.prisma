generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid()) @db.Uuid // Identificador único de usuario
  name      String // Nombre del usuario
  email     String   @unique // Correo electrónico único
  password  String // Contraseña del usuario
  createdAt DateTime @default(now()) // Fecha de creación del usuario
  updatedAt DateTime @updatedAt // Fecha de última actualización del usuario
}
